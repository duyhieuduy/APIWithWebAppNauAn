@model IEnumerable<APIWithWeb.Models.Congthucnguyenlieu>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Khoiluong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MamonNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManguyenlieuNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Khoiluong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MamonNavigation.Mamon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ManguyenlieuNavigation.Manguyenlieu)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ctnlid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ctnlid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ctnlid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
