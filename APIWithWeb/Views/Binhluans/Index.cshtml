@model IEnumerable<APIWithWeb.Models.Binhluan>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Noidungbl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MamonNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TendangnhapNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Noidungbl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MamonNavigation.Mamon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TendangnhapNavigation.Tendangnhap)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdBl">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdBl">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdBl">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
